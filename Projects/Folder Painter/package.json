{
  "name": "folder_changer",
  "version": "1.0.0",
  "description": "",
  "main": "Electron_Main.js",
  "scripts": {
    "start": "electron .",
    "package-mac": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=assets/icons/mac/icon.icns --prune=false --out=release-builds",
    "package-win": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=win32 --arch=ia32 --icon=assets/icons/win/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName=\"Folder Painter\"",
    "package-linux": "electron-packager . electron-tutorial-app --overwrite --asar=true --platform=linux --arch=x64 --icon=assets/icons/png/icon.png --prune=true --out=release-builds"

  },


  "author": "Michael",
  "license": "MIT",
  "dependencies": {
    "caman": "^4.1.2",
    "electron": "^18.3.7",
    "python": "0.0.4",
    "python-shell": "^2.0.1",
    "shell": "^0.5.0"
  },
  "devDependencies": {
    "electron-packager": "^15.0.0"
  }
}
